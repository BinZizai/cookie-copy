{
  "manifest_version": 3,
  "name": "Cookie复制助手",
  "version": "1.0",
  "description": "在不同网站之间复制粘贴 Cookie 和 Storage 数据",
  "permissions": [
    "cookies",
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/cookie2.png",
      "48": "images/cookie2.png",
      "128": "images/cookie2.png"
    }
  },
  "icons": {
    "16": "images/cookie2.png",
    "48": "images/cookie2.png",
    "128": "images/cookie2.png"
  }
} 